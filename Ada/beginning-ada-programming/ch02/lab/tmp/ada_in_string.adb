with Ada.Text_IO;

procedure ada_in_string is
  str : String :=   "###############################################################" & ASCII.LF
                  & "###############################################################" & ASCII.LF
		  & "##                                                           ##" & ASCII.LF
		  & "##       00000000000   0000000000    00000000000             ##" & ASCII.LF
		  & "##       00       00   00       00   00       00             ##" & ASCII.LF
		  & "##       00       00   00       00   00       00             ##" & ASCII.LF
		  & "##       00000000000   00       00   00000000000             ##" & ASCII.LF
		  & "##       00       00   00       00   00       00             ##" & ASCII.LF
		  & "##       00       00   00       00   00       00             ##" & ASCII.LF
		  & "##       00       00   0000000000    00       00             ##" & ASCII.LF
		  & "##                                                           ##" & ASCII.LF
		  & "###############################################################" & ASCII.LF
		  & "###############################################################";
begin
  Ada.Text_IO.Put_Line(str);
end ada_in_string;

